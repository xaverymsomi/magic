RewriteEngine On
RewriteBase /
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-l

RewriteRule ^(.+)$ index.php?url=$1 [QSA,L]
Header always set Content-Security-Policy " default-src https: 'unsafe-eval' 'unsafe-inline';img-src https: data:; object-src 'none';connect-src 'self' wss://socket.rahisi.co.tz:3721"
